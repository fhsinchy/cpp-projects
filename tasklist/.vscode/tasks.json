{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "Windows: cl.exe build tasklist project",
			"command": "cl.exe",
			"args": [
				"/Zi",
				"/EHsc",
				"/nologo",
				"/Fe${workspaceFolder}\\bin\\tasklist.exe",
				"${workspaceFolder}/src/*.cpp",
			],
			"problemMatcher": [
				"$msCompile"
			],
			"group": "build",
			"detail": "compiler: cl.exe"
		}
	]
}